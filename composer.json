{
    "name": "arkste/hetzner-cloud-client-php",
    "description": "Hetzner Cloud Client in PHP",
    "keywords": [
        "hetzner",
        "cloud",
        "hcloud",
        "client",
        "api"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arkadius Stefanski",
            "email": "arkste@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5.9|>=7.0.8",
        "psr/http-message": "^1.0",
        "php-http/client-common": "^1.6",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0",
        "php-http/cache-plugin": "^1.4",
        "symfony/options-resolver": "^2.6 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "php-http/guzzle6-adapter": "^1.0",
        "php-http/mock-client": "^1.0",
        "guzzlehttp/psr7": "^1.2",
        "php-http/message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "HetznerCloud\\": "src/HetznerCloud/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HetznerCloud\\Tests\\": "tests/HetznerCloud/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
